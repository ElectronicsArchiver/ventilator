                    android:minHeight="30dp"
                    android:text="CLOSE" />

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/btSnoozeLowPeepAlertDialog"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
